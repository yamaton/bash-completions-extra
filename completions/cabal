_cabal_help()
{
    local cmd i subcommands
    local subcommands=" cabal"

    for (( i=1; i < cword; i++ )); do
        if [[ " ${subcommands[*]} " == *" ${words[i]} "* ]]; then
            cmd=${words[i]}
            break
        fi
    done

    case "$cmd" in
        cabal) _cabal_help_cabal
            return
            ;;

    esac

    local word_list=" cabal -h --help"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

