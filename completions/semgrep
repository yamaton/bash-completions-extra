# Auto-generated with h2o

_semgrep()
{
    local cur prev words cword
    _init_completion -s || return

    local cmd i subcommands
    local subcommands=" ci install-semgrep-pro login logout lsp publish scan show test validate mcp"

    for (( i=1; i < cword; i++ )); do
        if [[ " ${subcommands[*]} " == *" ${words[i]} "* ]]; then
            cmd=${words[i]}
            break
        fi
    done

    case "$cmd" in
        ci) _semgrep_ci
            return
            ;;

        install-semgrep-pro) _semgrep_installsemgreppro
            return
            ;;

        login) _semgrep_login
            return
            ;;

        logout) _semgrep_logout
            return
            ;;

        lsp) _semgrep_lsp
            return
            ;;

        publish) _semgrep_publish
            return
            ;;

        scan) _semgrep_scan
            return
            ;;

        show) _semgrep_show
            return
            ;;

        test) _semgrep_test
            return
            ;;

        validate) _semgrep_validate
            return
            ;;

        mcp) _semgrep_mcp
            return
            ;;

    esac

    local word_list=" ci install-semgrep-pro login logout lsp publish scan show test validate mcp -h --help"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

_semgrep_ci()
{
    local word_list="  -a --autofix --allow-local-builds --allow-untrusted-validators --baseline-commit --code -d --dump-command-for-core --dataflow-traces --disable-nosem --disable-version-check --dry-run --dryrun --emacs --emacs-output --enable-nosem --enable-version-check --exclude --exclude-minified-files --exclude-rule -f -c --config --fake-backend --files-with-matches --force-color --gitlab-sast --gitlab-sast-output --gitlab-secrets --gitlab-secrets-output --historical-secrets --include --incremental-output --interfile-timeout --internal-ci-scan-results -j --jobs --json --json-output --junit-xml --junit-xml-output --log-backend --matching-explanations --max-chars-per-line --max-lines-per-finding --max-log-list-entries --max-memory --max-target-bytes --metrics --no-autofix --no-dryrun --no-exclude-minified-files --no-force-color --no-git-ignore --no-rewrite-rule-ids --no-secrets-validation --no-suppress-errors -o --output --optimizations --oss-only --pro --pro-intrafile --pro-languages --pro-path-sensitive --rewrite-rule-ids --sarif --sarif-output --scan-unknown-extensions --secrets --semgrepignore-v2 --skip-unknown-extensions --subdir --supply-chain --suppress-errors --text --text-output --timeout --timeout-threshold --use-git-ignore --vim --vim-output --debug --develop --experimental --help --legacy --no-trace --profile -q --quiet --trace --trace-endpoint -v --verbose --x-disable-transitive-reachability --x-dump-rule-partitions --x-dump-rule-partitions-dir --x-dump-rule-partitions-strategy --x-eio --x-ignore-semgrepignore-files --x-mcp --x-merge-partial-results-dir --x-merge-partial-results-output --x-no-python-schema-validation --x-parmap --x-partial-config --x-partial-output --x-pro-naming --x-semgrepignore-filename --x-simple-profiling --x-tr --x-enable-transitive-reachability --x-upload-partial-results --x-upload-partial-results-scan-id --x-validate-partial-results-actual --x-validate-partial-results-expected"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

