_semgrep_show()
{
    local cmd i subcommands
    local subcommands=" resources"

    for (( i=1; i < cword; i++ )); do
        if [[ " ${subcommands[*]} " == *" ${words[i]} "* ]]; then
            cmd=${words[i]}
            break
        fi
    done

    case "$cmd" in
        resources) _semgrep_show_resources
            return
            ;;

    esac

    local word_list=" resources --help"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

_semgrep_show_resources()
{
    local word_list="  --x-eio --x-no-python-schema-validation --x-parmap --x-simple-profiling --json --debug --develop --experimental --help --legacy --no-trace --profile -q --quiet --trace --trace-endpoint -v --verbose"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

