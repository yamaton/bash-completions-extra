{
    local word_list="  --direction --detect-string-imports --min-dots --preview --target-version --python --type-checking-imports -h --help -v --verbose -q --quiet -s --silent --config --isolated"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

_ruff_analyze_help()
{
    local word_list="  "
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

