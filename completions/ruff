_ruff_analyze()
{
    local cmd i subcommands
    local subcommands=" graph help"

    for (( i=1; i < cword; i++ )); do
        if [[ " ${subcommands[*]} " == *" ${words[i]} "* ]]; then
            cmd=${words[i]}
            break
        fi
    done

    case "$cmd" in
        graph) _ruff_analyze_graph
            return
            ;;

        help) _ruff_analyze_help
            return
            ;;

    esac

    local word_list=" graph help -h --help -v --verbose -q --quiet -s --silent --config --isolated"
    COMPREPLY=( $(compgen -W "${word_list}" -- "$cur") )
}

